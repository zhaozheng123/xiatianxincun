/**seeting.wxss**/
body{
  margin:0;
  padding:0;
}
.myMedia{
  position: absolute;
  width: 100%;
  height: 100%;
  left: 0;
  top: 0;
  background-color: #dbdbdb;
}
video{
  position: relative;
  width:750rpx;
  height:421rpx;
}
.bg_reserve{
  position: relative;
  width: 750rpx;
  height: 421rpx;
}
.summary{
  position: absolute;
  margin-top: 10rpx;
  width: 730rpx;
  left: 10rpx;
  height: 400rpx;
  background-color: #ffffff;
  border-radius: 10rpx;
}
.first_cow{
  position: relative;
  height: 100rpx;
  display: flex;
  align-items: center;
  margin-top: 20rpx;
}
.first_cow .btn_area{
  position: relative;
  width: 140rpx;
  height: 65rpx;
  margin-left: 20rpx;
  border-radius: 10rpx;
  overflow: hidden;
}
.btn_area image{
  position: relative;
  width: 100%;
  height: 100%;
}
.btn_area view{
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  text-align: center;
  height: 65rpx;
  line-height: 65rpx;
  color:#ffffff;
}
.tit{
  position: relative;
  margin-left: 40rpx;
  font-size: 1.5rem;
  color:#30beff;
  letter-spacing: 2.29rpx;
}
.cow{
  position: relative;
  width: 100%;
  height: 60rpx;
  display: flex;
  align-items: center;
  margin-top: 20rpx;
}
.cow image{
  width: 45rpx;
  height: 45rpx;
  margin-left: 60rpx;
}
.cow view{
  margin-left: 40rpx;
  height: 60rpx;
  line-height: 60rpx;
}


