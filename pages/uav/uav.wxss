body{
  padding: 0;
  margin: 0;
  color: #333333;
}
.page{ 
  height: 100%;
  width: 100%;
  position: absolute;
  left: 0;
  top: 0;
  display: flex;
  flex-direction: column;
  /* justify-content: center; */
  align-items: center;
}
.mainBg{
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0;
  top: 0;
}
.toshoot{
  width: 70%;
  height: 180rpx;
  background: white;
  position: relative;
  border-radius: 23px;
  box-shadow: 0px 3px 6px #000000;
  display: flex;
  align-items: center;
  font-size: 1rem;
  color: #333333;
  font-weight: bolder;
  margin-top: 160rpx;
}
.toshoot:active{
  transform: scale(0.95);
  box-shadow: 0px 0px 20px #5b9df3;
  transition-duration: 0.2s;
}
.ExempleImg{
  width: 40%;
  height: 100%;
  display: flex;
  align-items: center;
  margin-left: 50rpx;
}
.item_img{
  width:80%;
  height: 80%;
}
.toseemine{
  width: 70%;
  height: 180rpx;
  background: white;
  position: relative;
  border-radius: 23px;
  box-shadow: 0px 3px 6px #000000;
  display: flex;
  align-items: center;
  font-size: 1rem;
  color: #333333;
  font-weight: bolder;
  margin-top: 30rpx;
}
.toseemine:active{
  transform: scale(0.95);
  box-shadow: 0px 0px 20px #5b9df3;
  transition-duration: 0.2s;
}